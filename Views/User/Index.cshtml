@{
    ViewData["Title"] = "Home Page";
}

@model List<User>
<div class="text-center m-5">
    @await Html.PartialAsync("_navbar")
</div>
<div class="contaier text-center">
    <h1>Check out our roster of chefs!</h1>
    <hr>
    <table class="table table-striped">
        <thead>
            <tr>
                <td>Name</td>
                <td>Age</td>
                <td># of Dishes</td>
            </tr>
        </thead>
        @foreach (User p in Model)
        {
            <tbody>
                <tr>
                    <td>@p.FirstName @p.LastName</td>
                    <td>@p.Birthday</td>
                    <td>@p.AllDishes.Count()</td>
                </tr>
            </tbody>

        }

    </table>
</div>
